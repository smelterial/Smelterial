{
  "name": "@smelterial/you-icon",
  "license": "GPL-3.0",
  "version": "0.4.0",
  "description": "Easy access to Google's Material Symbols. A simple API, fully typed right, down to the icon names.",
  "type": "module",
  "scripts": {
    "build": "svelte-package",
    "dev": "vite dev",
    "lint": "prettier --ignore-path ../../../../.prettierignore --check .",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "sync": "svelte-kit sync"
  },
  "keywords": [
    "smelterial",
    "svelte",
    "component",
    "material you",
    "material design",
    "icon"
  ],
  "exports": {
    ".": {
      "svelte": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./Icon.svelte": {
      "svelte": "./dist/Icon.svelte",
      "types": "./dist/Icon.svelte.d.ts"
    }
  },
  "dependencies": {
    "svelte": "^3.59.1"
  },
  "devDependencies": {
    "@smelterial/shared-config": "workspace:*",
    "@sveltejs/kit": "^1.20.2",
    "@sveltejs/package": "^2.0.2",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "sass": "^1.63.3",
    "svelte-check": "^3.4.3",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3",
    "vite": "^4.3.9"
  }
}
